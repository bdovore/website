<h2>Cache - Tomes similaires</h2>
<button id="lancerSimil">lancer</button>

<form id="cacheSimil" name="cacheSimil" method="get">
<input type="hidden" name="continuer" value="">
</form>

<div id='ddcacheSimil'>vide</div>
<script>
  $(function() {
        $( "#lancerSimil" ).button({
            icons: {
              primary: "ui-icon-play"
            }
          })
          .click(function() {
            var options;
            if ( $( this ).text() === "lancer" ) {
                document.cacheSimil.continuer.value = "oui";
                $.get("<?php echo BDO_URL;?>cache/simil", $("#cacheSimil").serialize(), function(data) {
                     $("#ddcacheSimil").html(data);
                    //alert("Data Loaded: " + data);
                    });
              options = {
                label: "pause",
                icons: {
                  primary: "ui-icon-pause"
                }
              };
            } else {
                document.cacheSimil.continuer.value = "";
            options = {
                label: "lancer",
                icons: {
                  primary: "ui-icon-play"
                }
              };
            }
            $( this ).button( "option", options );
          })
  });
  </script>

